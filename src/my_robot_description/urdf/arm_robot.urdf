            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <!--7.grabber_link_garbber_right_joint-->
    <transmission name="trans_grabber_link_garbber_right_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="grabber_link_garbber_right_joint">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="grabber_link_garbber_right_joint_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <gazebo>
        <plugin name="arm_controller" filename="libgazebo_ros_control.so">
            <robotNamespace>/</robotNamespace>
        </plugin>
        <plugin name="gazebo_ros_control">
            <robotNamespace>/</robotNamespace>
        </plugin>
    </gazebo>
    <!--JOINT LIST 
    	1. base_link_cyclnder_joint
    	2. cylinder_link_first_box_joint
    	3. first_box_second_box_joint
    	4. second_box_third_box_joint
    	5. third_box_grabber_joint
    	6. grabber_link_garbber_left_joint
    	7. grabber_link_garbber_right_joint
    	-->
    <gazebo reference="base_link_cyclnder_joint">
        <selfCollide>true</selfCollide>
    </gazebo>
    <gazebo reference="cylinder_link_first_box_joint">
        <selfCollide>true</selfCollide>
    </gazebo>
    <gazebo reference="first_box_second_box_joint">
