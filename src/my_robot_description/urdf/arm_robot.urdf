            </geometry>
        </collision>
        <inertial>
            <mass value="0.100" />
            <origin xyz="0 0 0.4" rpy="0 0 0" />
            <inertia ixx="0.00013" ixy="0" ixz="0" iyy="0.00013" iyz="0" izz="0.00008" />
        </inertial>
  
    </link>
    <link name="cylinder_link">
