    </link>
    <!--JOINTS-->
    <joint name="ground_base_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="ground" />
        <child link="base_link" />
    </joint>
    <joint name="base_link_cyclnder_joint" type="revolute">
        <origin xyz="0 0 0.083" rpy="0 0 0" />
        <parent link="base_link" />
        <child link="cylinder_link" />
        <axis xyz="0 0 1" />
        <limit effort="30" velocity="1.0" lower="-1.57" upper="1.57" />
    </joint>
    <joint name="cylinder_link_first_box_joint" type="revolute">
        <origin xyz="0 0 0.007" rpy="0 0 0" />
        <parent link="cylinder_link" />
        <child link="first_box_link" />
        <axis xyz="1 0 0" />
        <limit effort="30" velocity="1.0" lower="-1.57" upper="1.57" />
    </joint>
    <joint name="first_box_second_box_joint" type="revolute">
        <origin xyz="0.021 0 0.075" rpy="0 0 0" />
        <parent link="first_box_link" />
        <child link="second_box_link" />
        <axis xyz="1 0 0" />
        <limit effort="30" velocity="1.0" lower="-1.57" upper="1.57" />
    </joint>
    <joint name="second_box_third_box_joint" type="revolute">
        <origin xyz="0.021 0 0.075" rpy="0 0 0" />
        <parent link="second_box_link" />
        <child link="third_box_link" />
        <axis xyz="1 0 0" />
        <limit effort="30" velocity="1.0" lower="-1.57" upper="1.57" />
    </joint>
    <joint name="third_box_grabber_joint" type="revolute">
        <origin xyz="0 0 0.085" rpy="0 0 0" />
        <parent link="third_box_link" />
        <child link="grabber_link" />
        <axis xyz="0 0 1" />
        <limit effort="30" velocity="1.0" lower="-1.57" upper="1.57" />
    </joint>
    <joint name="grabber_link_garbber_left_joint" type="prismatic">
        <origin xyz="0 0.01 0.04" rpy="0 0 0" />
        <parent link="grabber_link" />
        <child link="grabber_left" />
        <axis xyz="0 1 0" />
        <limit effort="30" velocity="1.0" lower="0.0" upper="0.03" />
    </joint>
    <joint name="grabber_link_garbber_right_joint" type="prismatic">
        <origin xyz="0 -0.01 0.040" rpy="0 0 0" />
        <parent link="grabber_link" />
        <child link="grabber_right" />
        <axis xyz="0 1 0" />
        <limit effort="30" velocity="1.0" lower="0.0" upper="-0.03" />
    </joint>
    <!--transmissions-->
    <!--JOINT LIST 
    	1. base_link_cyclnder_joint
    	2. cylinder_link_first_box_joint
    	3. first_box_second_box_joint
    	4. second_box_third_box_joint
    	5. third_box_grabber_joint
    	6. grabber_link_garbber_left_joint
    	7. grabber_link_garbber_right_joint
    	-->
    <!--1.base_link_cyclnder_joint-->
    <transmission name="trans_base_link_cyclnder_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="base_link_cyclnder_joint">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="second_box_third_box_joint_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <!--2.cylinder_link_first_box_joint-->
    <transmission name="trans_cylinder_link_first_box_joint">
        <type>transmission_interface/SimpleTransmission</type>
